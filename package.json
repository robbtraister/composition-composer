  "files": [
    "bin",
    "build",
    "project/manifest",
    "src",
    "webpack",
    "aliases.js",
    "babel.config.json",
    "env.js"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/server/*.ts",
      "!src/server/router/auth/**/*",
      "!**/*.d.ts"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "^~/(.*)": "<rootDir>/project/src/$1"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "/node_modules/"
    ],
    "watchPathIgnorePatterns": [
      "<rootDir>/build/",
      "/node_modules/"
    ]
  },
  "lint-staged": {
    "**/*.{css,sass,scss}": [
      "prettier",
      "git add"
    ],
    "**/*.{cjs,cjsx,js,jsx,mjs,mjsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "prettier": "prettier-config-standard"
